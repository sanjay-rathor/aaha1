{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]",
          "meta[name=theme-color]"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter"
        ]
      }
    ],
    "no-inline-styles": "off",
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ],
    "meta-viewport": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ],
    "detect-css-reflows/composite": "off",
    "axe/aria": [
      "default",
      {
        "aria-allowed-attr": "off"
      }
    ]
  }
}